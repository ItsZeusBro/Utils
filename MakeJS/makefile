DeveloperCToolsAutomata: ${DEVELOPER_CTOOLS_AUTOMATA_TEST_C_DEPENDENCIES} ${DEVELOPER_CTOOLS_AUTOMATA_TEST_H_DEPENDENCIES}
	cd ${CTOOLS_AUTOMATA_DIR}; gcc -c ${CTOOLS_AUTOMATA_C}
	cd ${CTOOLS_AUTOMATA_TEST_DIR}; gcc -c ${CTOOLS_AUTOMATA_TEST_C} ${CTOOLS_AUTOMATA_TEST_DRIVER_C}

ProductionCToolsAutomata: ${PRODUCTION_CTOOLS_AUTOMATA_TEST_C_DEPENDENCIES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_H_DEPENDENCIES}
	cd ${CTOOLS_AUTOMATA_DIR}; gcc -c ${CTOOLS_AUTOMATA_C}
	cd ${CTOOLS_AUTOMATA_TEST_DIR}; gcc -c ${CTOOLS_AUTOMATA_TEST_C}

DeveloperCToolsAutomataClean: ${DEVELOPER_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES}
	cd ${CTOOLS_AUTOMATA_DIR}; rm -f ${CTOOLS_AUTOMATA_O}
	cd ${CTOOLS_AUTOMATA_TEST_DIR}; rm -f ${CTOOLS_AUTOMATA_TEST_O} ${CTOOLS_AUTOMATA_TEST_DRIVER_O}

ProductionCToolsAutomataClean: ${PRODUCTION_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES}
	cd ${CTOOLS_AUTOMATA_DIR}; rm -f ${CTOOLS_AUTOMATA_O}
	cd ${CTOOLS_AUTOMATA_TEST_DIR}; rm -f ${CTOOLS_AUTOMATA_TEST_O}

DeveloperCToolsAutomataLink: ${DEVELOPER_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES}
	(gcc -o developerTest ${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH} ${CTOOLS_AUTOMATA_TEST_DRIVER_O_PATH})

ProductionCToolsAutomataLink: ${PRODUCTION_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES}
	(gcc -o productionTest ${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH})

DeveloperAutomataRun: ${DEVELOPER_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES} ${DEVELOPER_CTOOLS_AUTOMATA_TEST_C_DEPENDENCIES} ${DEVELOPER_CTOOLS_AUTOMATA_TEST_H_DEPENDENCIES}
	make DeveloperAutomataClean
	make DeveloperAutomata
	make DeveloperAutomataLink
	./developerTest

