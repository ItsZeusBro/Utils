	cd ${CTOOLS_UTILS_ASSERTIONS_TEST_DIR}; gcc -c ${CTOOLS_UTILS_ASSERTIONS_TEST_C} ${CTOOLS_UTILS_ASSERTIONS_TEST_DRIVER_C}

ProductionCToolsUtilsAssertions: ${PRODUCTION_CTOOLS_UTILS_ASSERTIONS_TEST_C_DEPENDENCIES} ${PRODUCTION_CTOOLS_UTILS_ASSERTIONS_TEST_H_DEPENDENCIES}
	cd ${CTOOLS_UTILS_ASSERTIONS_DIR}; gcc -c ${CTOOLS_UTILS_ASSERTIONS_C}
	cd ${CTOOLS_UTILS_ASSERTIONS_TEST_DIR}; gcc -c ${CTOOLS_UTILS_ASSERTIONS_TEST_C}

DeveloperCToolsUtilsAssertionsClean: ${DEVELOPER_CTOOLS_UTILS_ASSERTIONS_TEST_O_DEPENDENCIES}
	cd ${CTOOLS_UTILS_ASSERTIONS_DIR}; rm -f ${CTOOLS_UTILS_ASSERTIONS_O}
	cd ${CTOOLS_UTILS_ASSERTIONS_TEST_DIR}; rm -f ${CTOOLS_UTILS_ASSERTIONS_TEST_O} ${CTOOLS_UTILS_ASSERTIONS_TEST_DRIVER_O}

ProductionCToolsUtilsAssertionsClean: ${PRODUCTION_CTOOLS_UTILS_ASSERTIONS_TEST_O_DEPENDENCIES}
	cd ${CTOOLS_UTILS_ASSERTIONS_DIR}; rm -f ${CTOOLS_UTILS_ASSERTIONS_O}
	cd ${CTOOLS_UTILS_ASSERTIONS_TEST_DIR}; rm -f ${CTOOLS_UTILS_ASSERTIONS_TEST_O}

DeveloperCToolsUtilsAssertionsLink: ${DEVELOPER_CTOOLS_UTILS_ASSERTIONS_TEST_O_DEPENDENCIES}
	(gcc -o developerTest ${CTOOLS_UTILS_ASSERTIONS_O_PATH} ${CTOOLS_UTILS_ASSERTIONS_TEST_O_PATH} ${CTOOLS_UTILS_ASSERTIONS_TEST_DRIVER_O_PATH})

ProductionCToolsUtilsAssertionsLink: ${PRODUCTION_CTOOLS_UTILS_ASSERTIONS_TEST_O_DEPENDENCIES}
	(gcc -o productionTest ${CTOOLS_UTILS_ASSERTIONS_O_PATH} ${CTOOLS_UTILS_ASSERTIONS_TEST_O_PATH})

DeveloperCToolsUtilsAssertionsRun: ${DEVELOPER_CTOOLS_UTILS_ASSERTIONS_TEST_O_DEPENDENCIES} ${DEVELOPER_CTOOLS_UTILS_ASSERTIONS_TEST_C_DEPENDENCIES} ${DEVELOPER_CTOOLS_UTILS_ASSERTIONS_TEST_H_DEPENDENCIES}
	make DeveloperCToolsUtilsAssertionsClean
	make DeveloperCToolsUtilsAssertions
	make DeveloperCToolsUtilsAssertionsLink
	./developerTest

