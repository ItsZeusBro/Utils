CTOOLS_DIR=../CTools/
CTOOLS_TEST_DIR=${CTOOLS_DIR}Test/
CTOOLS_C=CTools.c
CTOOLS_H=CTools.h
CTOOLS_O=CTools.o
CTOOLS_TEST_C=Test.c
CTOOLS_TEST_H=Test.h
CTOOLS_TEST_O=Test.o
CTOOLS_TEST_DRIVER_C=Driver.c
CTOOLS_TEST_DRIVER_H=Driver.h
CTOOLS_TEST_DRIVER_O=Driver.o
CTOOLS_C_PATH=${CTOOLS_DIR}${CTOOLS_C}
CTOOLS_H_PATH=${CTOOLS_DIR}${CTOOLS_H}
CTOOLS_O_PATH=${CTOOLS_DIR}${CTOOLS_O}
CTOOLS_TEST_C_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_C}
CTOOLS_TEST_H_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_H}
CTOOLS_TEST_O_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_O}
CTOOLS_TEST_DRIVER_C_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_DRIVER_C}
CTOOLS_TEST_DRIVER_H_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_DRIVER_H}
CTOOLS_TEST_DRIVER_O_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_DRIVER_O}
CTOOLS_TEST_DRIVER_C_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_DRIVER_C}
CTOOLS_TEST_DRIVER_H_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_DRIVER_H}
CTOOLS_TEST_DRIVER_O_PATH=${CTOOLS_TEST_DIR}${CTOOLS_TEST_DRIVER_O}
PRODUCTION_CTOOLS_TEST_C_DEPENDENCIES=${CTOOLS_C_PATH} ${CTOOLS_TEST_C_PATH} 
PRODUCTION_CTOOLS_TEST_H_DEPENDENCIES=${CTOOLS_H_PATH} ${CTOOLS_TEST_H_PATH} 
PRODUCTION_CTOOLS_TEST_O_DEPENDENCIES=${CTOOLS_O_PATH} ${CTOOLS_TEST_O_PATH} 
DEVELOPER_CTOOLS_TEST_C_DEPENDENCIES=${CTOOLS_C_PATH} ${CTOOLS_TEST_C_PATH} ${CTOOLS_TEST_DRIVER_C_PATH}
DEVELOPER_CTOOLS_TEST_H_DEPENDENCIES=${CTOOLS_H_PATH} ${CTOOLS_TEST_H_PATH} ${CTOOLS_TEST_DRIVER_H_PATH}
DEVELOPER_CTOOLS_TEST_O_DEPENDENCIES=${CTOOLS_O_PATH} ${CTOOLS_TEST_O_PATH} ${CTOOLS_TEST_DRIVER_O_PATH}
DEVELOPER_CTOOLS_TEST_C_FILES=${CTOOLS_C_PATH} ${CTOOLS_TEST_C_PATH} ${CTOOLS_TEST_DRIVER_C_PATH} 
DEVELOPER_CTOOLS_TEST_H_FILES=${CTOOLS_H_PATH} ${CTOOLS_TEST_H_PATH} ${CTOOLS_TEST_DRIVER_H_PATH} 
DEVELOPER_CTOOLS_TEST_O_FILES=${CTOOLS_O_PATH} ${CTOOLS_TEST_O_PATH} ${CTOOLS_TEST_DRIVER_O_PATH} 
PRODUCTION_CTOOLS_TEST_C_FILES=${CTOOLS_C_PATH} ${CTOOLS_TEST_C_PATH} 
PRODUCTION_CTOOLS_TEST_H_FILES=${CTOOLS_H_PATH} ${CTOOLS_TEST_H_PATH} 
PRODUCTION_CTOOLS_TEST_O_FILES=${CTOOLS_O_PATH} ${CTOOLS_TEST_O_PATH} 
CTOOLS_TEST_DEVELOPER_FILES= ${DEVELOPER_CTOOLS_TEST_C_FILES} ${DEVELOPER_CTOOLS_TEST_H_FILES} ${DEVELOPER_CTOOLS_TEST_O_FILES} 


DeveloperCTools: ${DEVELOPER_CTOOLS_TEST_C_DEPENDENCIES} ${DEVELOPER_CTOOLS_TEST_H_DEPENDENCIES}
	cd ${CTOOLS_DIR}; gcc -c ${CTOOLS_C}
	cd ${CTOOLS_TEST_DIR}; gcc -c ${CTOOLS_TEST_C} ${CTOOLS_TEST_DRIVER_C}

ProductionCTools: ${PRODUCTION_CTOOLS_TEST_C_DEPENDENCIES} ${PRODUCTION_CTOOLS_TEST_H_DEPENDENCIES}
	cd ${CTOOLS_DIR}; gcc -c ${CTOOLS_C}
	cd ${CTOOLS_TEST_DIR}; gcc -c ${CTOOLS_TEST_C}

DeveloperCToolsClean: ${DEVELOPER_CTOOLS_TEST_O_DEPENDENCIES}
	cd ${CTOOLS_DIR}; rm -f ${CTOOLS_O}
	cd ${CTOOLS_TEST_DIR}; rm -f ${CTOOLS_TEST_O} ${CTOOLS_TEST_DRIVER_O}

ProductionCToolsClean: ${PRODUCTION_CTOOLS_TEST_O_DEPENDENCIES}
	cd ${CTOOLS_DIR}; rm -f ${CTOOLS_O}
	cd ${CTOOLS_TEST_DIR}; rm -f ${CTOOLS_TEST_O}

DeveloperCToolsLink: ${DEVELOPER_CTOOLS_TEST_O_DEPENDENCIES}
	(gcc -o developerTest ${CTOOLS_O_PATH} ${CTOOLS_TEST_O_PATH} ${CTOOLS_TEST_DRIVER_O_PATH})

ProductionCToolsLink: ${PRODUCTION_CTOOLS_TEST_O_DEPENDENCIES}
	(gcc -o productionTest ${CTOOLS_O_PATH} ${CTOOLS_TEST_O_PATH})

DeveloperCToolsRun: ${DEVELOPER_CTOOLS_TEST_O_DEPENDENCIES} ${DEVELOPER_CTOOLS_TEST_C_DEPENDENCIES} ${DEVELOPER_CTOOLS_TEST_H_DEPENDENCIES}
	make DeveloperCToolsClean
	make DeveloperCTools
	make DeveloperCToolsLink
	./developerTest

