CTOOLS_AUTOMATA_TEST_DRIVER_O_PATH=${CTOOLS_AUTOMATA_TEST_DIR}${CTOOLS_AUTOMATA_TEST_DRIVER_O}
PRODUCTION_CTOOLS_AUTOMATA_TEST_C_DEPENDENCIES=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} 
PRODUCTION_CTOOLS_AUTOMATA_TEST_H_DEPENDENCIES=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} 
PRODUCTION_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES=${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH} 
DEVELOPER_CTOOLS_AUTOMATA_TEST_C_DEPENDENCIES=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} ${CTOOLS_AUTOMATA_TEST_DRIVER_C_PATH}
DEVELOPER_CTOOLS_AUTOMATA_TEST_H_DEPENDENCIES=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} ${CTOOLS_AUTOMATA_TEST_DRIVER_H_PATH}
DEVELOPER_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES=${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH} ${CTOOLS_AUTOMATA_TEST_DRIVER_O_PATH}
DEVELOPER_CTOOLS_AUTOMATA_TEST_C_FILES=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} ${CTOOLS_AUTOMATA_TEST_DRIVER_C_PATH} 
DEVELOPER_CTOOLS_AUTOMATA_TEST_H_FILES=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} ${CTOOLS_AUTOMATA_TEST_DRIVER_H_PATH} 
DEVELOPER_CTOOLS_AUTOMATA_TEST_O_FILES=${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH} ${CTOOLS_AUTOMATA_TEST_DRIVER_O_PATH} 
PRODUCTION_CTOOLS_AUTOMATA_TEST_C_FILES=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} 
PRODUCTION_CTOOLS_AUTOMATA_TEST_H_FILES=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} 
