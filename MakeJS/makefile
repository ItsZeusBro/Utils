CTOOLS_AUTOMATA_TEST_DRVR_O_PATH=${CTOOLS_AUTOMATA_TEST_DIR}${CTOOLS_AUTOMATA_TEST_DRVR_O}
PROD_CTOOLS_AUTOMATA_TEST_C_DEPS=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} 
PROD_CTOOLS_AUTOMATA_TEST_H_DEPS=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} 
PROD_CTOOLS_AUTOMATA_TEST_O_DEPS=${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH} 
DEV_CTOOLS_AUTOMATA_TEST_C_DEPS=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} ${CTOOLS_AUTOMATA_TEST_DRVR_C_PATH} 
DEV_CTOOLS_AUTOMATA_TEST_H_DEPS=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} ${CTOOLS_AUTOMATA_TEST_DRVR_H_PATH} 
DEV_CTOOLS_AUTOMATA_TEST_O_DEPS=${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH} ${CTOOLS_AUTOMATA_TEST_DRVR_O_PATH} 
DEV_CTOOLS_AUTOMATA_TEST_C_FILES=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} ${CTOOLS_AUTOMATA_TEST_DRVR_C_PATH} 
DEV_CTOOLS_AUTOMATA_TEST_H_FILES=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} ${CTOOLS_AUTOMATA_TEST_DRVR_H_PATH} 
DEV_CTOOLS_AUTOMATA_TEST_O_FILES=${CTOOLS_AUTOMATA_O_PATH} ${CTOOLS_AUTOMATA_TEST_O_PATH} ${CTOOLS_AUTOMATA_TEST_DRVR_O_PATH} 
PROD_CTOOLS_AUTOMATA_TEST_C_FILES=${CTOOLS_AUTOMATA_C_PATH} ${CTOOLS_AUTOMATA_TEST_C_PATH} 
PROD_CTOOLS_AUTOMATA_TEST_H_FILES=${CTOOLS_AUTOMATA_H_PATH} ${CTOOLS_AUTOMATA_TEST_H_PATH} 
