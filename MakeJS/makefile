ProductionTests: ${PRODUCTION_CTOOLS_TEST_C_DEPENDENCIES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_C_DEPENDENCIES}  ${PRODUCTION_CTOOLS_TEST_H_DEPENDENCIES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_H_DEPENDENCIES} 
	make ProductionCTools
	make ProductionCToolsAutomata


ProductionTestsLink: ${PRODUCTION_CTOOLS_TEST_O_DEPENDENCIES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES} 
	gcc -o Production ${PRODUCTION_CTOOLS_TEST_O_FILES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_O_FILES} 

ProductionTestsClean: ${PRODUCTION_CTOOLS_TEST_O_DEPENDENCIES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_O_DEPENDENCIES} 
	make ProductionCToolsClean
	make ProductionCToolsAutomataClean


ProductionTestsRun: ${PRODUCTION_CTOOLS_TEST_C_DEPENDENCIES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_C_DEPENDENCIES}  ${PRODUCTION_CTOOLS_TEST_H_DEPENDENCIES} ${PRODUCTION_CTOOLS_AUTOMATA_TEST_H_DEPENDENCIES} 
	make ProductionTestsClean
	make ProductionTests
	make ProductionTestsLink
	./Production

