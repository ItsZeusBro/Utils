UTILS_TYPES_ARRAY_TEST_DRIVER_o=driver.o
UTILS_TYPES_ARRAY_TEST_DRIVER_c_PATH=${UTILS_TYPES_ARRAY_TEST_DIR}${UTILS_TYPES_ARRAY_TEST_DRIVER_c}
UTILS_TYPES_ARRAY_TEST_DRIVER_o_PATH=${UTILS_TYPES_ARRAY_TEST_DIR}${UTILS_TYPES_ARRAY_TEST_DRIVER_o}

UTILS_TYPES_ARRAY_DEVELOPER_TEST_c_DEPENDENCIES=${UTILS_TYPES_ARRAY_c_PATH} ${UTILS_TYPES_ARRAY_TEST_c_PATH} ${UTILS_TYPES_ARRAY_TEST_DRIVER_c_PATH}
UTILS_TYPES_ARRAY_DEVELOPER_TEST_o_DEPENDENCIES=${UTILS_TYPES_ARRAY_o_PATH} ${UTILS_TYPES_ARRAY_TEST_o_PATH} ${UTILS_TYPES_ARRAY_TEST_DRIVER_o_PATH}

UTILS_TYPES_ARRAY_PRODUCTION_TEST_c_DEPENDENCIES=${UTILS_TYPES_ARRAY_c_PATH} ${UTILS_TYPES_ARRAY_TEST_c_PATH}
UTILS_TYPES_ARRAY_PRODUCTION_TEST_o_DEPENDENCIES=${UTILS_TYPES_ARRAY_o_PATH} ${UTILS_TYPES_ARRAY_TEST_o_PATH}


utilsTypesArrayDev: ${UTILS_TYPES_ARRAY_DEVELOPER_TEST_c_DEPENDENCIES}
	(cd ${UTILS_TYPES_ARRAY_DIR}; gcc -c ${UTILS_TYPES_ARRAY_c})
	(cd ${UTILS_TYPES_ARRAY_TEST_DIR}; gcc -c ${UTILS_TYPES_ARRAY_TEST_c} ${UTILS_TYPES_ARRAY_TEST_DRIVER_c})

utilsTypesArrayDevClean: ${UTILS_TYPES_ARRAY_DEVELOPER_TEST_o_DEPENDENCIES}
	(cd ${UTILS_TYPES_ARRAY_DIR}; rm ${UTILS_TYPES_ARRAY_o})
	(cd ${UTILS_TYPES_ARRAY_TEST_DIR}; rm ${UTILS_TYPES_ARRAY_TEST_o} ${UTILS_TYPES_ARRAY_TEST_DRIVER_o})

utilsTypesArrayDevLink: ${UTILS_TYPES_ARRAY_DEVELOPER_TEST_o_DEPENDENCIES}
	(cd ${UTILS_TYPES_ARRAY_TEST_DIR}; gcc -o output ${UTILS_TYPES_ARRAY_TEST_o} ${UTILS_TYPES_ARRAY_TEST_DRIVER_o} ../${UTILS_TYPES_ARRAY_o})

utilsTypesArrayDevRun: ${UTILS_TYPES_ARRAY_DEVELOPER_TEST_c_DEPENDENCIES} ${UTILS_TYPES_ARRAY_DEVELOPER_TEST_o_DEPENDENCIES}
	(make utilsTypesArrayDevClean)
	(make utilsTypesArrayDev)
	(make utilsTypesArrayDevLink)
	(cd ${UTILS_TYPES_ARRAY_TEST_DIR}; ./output)



